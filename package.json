{
  "name": "react-sheets",
  "description": "A Spreadsheet Component for ReactJS",
  "homepage": "https://github.com/patrick-jones/react-sheets",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/patrick-jones/react-sheets.git"
  },
  "author": "Patrick Jones <pjnola@gmail.com> (https://github.com/patrick-jones)",
  "contributors": [
    "Patrick Jones <pjnola@gmail.com> (https://github.com/patrick-jones)",
    "Felix Rieseberg <felix@felixrieseberg.com> (http://www.felixrieseberg.com)"
  ],
  "dependencies": {
    "classnames": "^2.2.5",
    "core-js": "^2.4.1"
  },
  "peerDependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "cpx": "^1.5.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "eslint": "^3.10.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.6.0",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.13",
    "postcss": "^5.2.5",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^1.1.1",
    "precss": "^1.4.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "stylelint-config-standard": "^14.0.0",
    "webpack": "^1.13.3"
  },
  "scripts": {
    "build:lib": "babel ./src --out-dir ./lib && cpx './src/**/*.css' ./lib",
    "build:umd": "cross-env NODE_ENV=development webpack --config ./webpack.config.js --colors --profile --progress src/index.js dist/react-sheets.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --colors --profile --progress src/index.js dist/react-sheets.min.js",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "css": "cpx './src/**/*.css' ./lib",
    "clean": "rimraf ./lib ./dist",
    "rebuild": "npm run clean && npm run build",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:js": "eslint ./src --ext .js",
    "test": "cross-env NODE_ENV=test karma start",
    "test:watch": "cross-env NODE_ENV=test karma start --single-run",
    "example": "cross-env NODE_ENV=development webpack-dev-server --config ./example/webpack.config.js --content-base ./example --hot --inline --progress --display-error-details"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "main": "./lib/index.js"
}
